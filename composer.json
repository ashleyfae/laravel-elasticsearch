{
    "name": "ashleyfae/laravel-elasticsearch",
    "description": "Elasticsearch query builder for Laravel.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ashleyfae\\LaravelElasticsearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ashleyfae\\LaravelElasticsearch\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1",
        "elasticsearch/elasticsearch": "dev-master"
    },
    "require-dev": {
        "orchestra/testbench": "^7.1.0",
        "mockery/mockery": "^1.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\Ashleyfae\\LaravelElasticsearch\\ElasticServiceProvider"
            ]
        }
    }
}
